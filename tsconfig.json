{
    "compilerOptions": {
      "target": "es5",
      "allowJs": false,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true,
      "downlevelIteration": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }